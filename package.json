    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "bun": "^1.0.26",
    "dayjs": "^1.11.10",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1"
