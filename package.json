    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@vitejs/plugin-react": "^4.1.0",
    "bun": "^1.0.4",
    "dayjs": "^1.11.10",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "1.8.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
